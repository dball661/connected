{
  "name": "cdk",
  "version": "0.1.0",
  "bin": {
    "cdk": "bin/cdk.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk-synth": "cd lambda && npm run compile && cd ../ && cdk diff && cdk synth",
    "cdk-deploy": "cd lambda && npm run compile && cd ../ && cdk deploy --profile test"
  },
  "devDependencies": {
    "aws-cdk": "1.122.0",
    "@aws-cdk/assert": "1.122.0",
    "@types/jest": "^26.0.10",
    "@types/node": "10.17.27",
    "jest": "^26.4.2",
    "ts-jest": "^26.2.0",
    "ts-node": "^9.0.0",
    "typescript": "~3.9.7"
  },
  "dependencies": {
    "@aws-cdk/aws-sns": "1.122.0",
    "@aws-cdk/aws-sns-subscriptions": "1.122.0",
    "@aws-cdk/aws-sqs": "1.122.0",
    "@aws-cdk/aws-lambda": "1.122.0",
    "@aws-cdk/aws-apigateway": "1.122.0",
    "@aws-cdk/core": "1.122.0"
  }
}
